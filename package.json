{
  "name": "HoneyBadger",
  "version": "1.0.0",
  "description": "Batch file actions, (rename, image resize, zipping, send to ftp, you name it)",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "concurrently": "^6.0.2",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sharp": "^0.28.1",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "ebuild": "npm run build && node_modules/.bin/build",
    "test": "jest --verbose",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShawnCockburn/HoneyBadger.git"
  },
  "bugs": {
    "url": "https://github.com/ShawnCockburn/HoneyBadger/issues"
  },
  "homepage": "./",
  "main": "./electron/main.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.0"
  }
}
